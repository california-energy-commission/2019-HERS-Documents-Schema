<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.lmonte.com/besm/NRCVPLB22H" version="2019.1.002" xmlns="http://www.lmonte.com/besm/NRCVPLB22H" xmlns:altova="http://www.altova.com/xml-schema-extensions" xmlns:bld="http://www.lmonte.com/besm/bld" xmlns:com="http://www.lmonte.com/besm/com" xmlns:comp="http://www.lmonte.com/besm/comp" xmlns:d="http://www.lmonte.com/besm/d" xmlns:dtyp="http://www.lmonte.com/besm/dtyp" xmlns:env="http://www.lmonte.com/besm/env" xmlns:hvac="http://www.lmonte.com/besm/hvac" xmlns:lit="http://www.lmonte.com/besm/lit" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/ResBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/ResCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/ResCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/ResEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/ResHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/ResLighting.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTag" fixed="NRCVPLB22H"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentType" fixed="CERTIFICATE OF VERIFICATION"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleRes" fixed="HERS Verified High Rise Residential/Hotel/Motel Single Dwelling Unit Hot Water System Distribution"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderCF2R_3R"/>
                     <xsd:element ref="nRCVPLB22H"/>
                     <xsd:element name="DocAuthor" type="comp:DocumentAuthor"/>
                     <xsd:element name="RespPerson" type="comp:ResponsiblePersonNRCV"/>
                     <xsd:element name="Footer" type="comp:Footer"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20191201"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTag" fixed="NRCVPLB22H"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="NRCVPLB22H">
      <xsd:annotation>
         <xsd:documentation>HERS Verified High Rise Residential/Hotel/Motel Single Dwelling Unit Hot Water System Distribution</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">General Information</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">This table reports the dwelling unit name that were specified on the NRCC-PRF-01 compliance document for this project.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A01_ResidentialDwellingUnitName" type="com:ResidentialDwellingUnitName">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Dwelling Unit Name</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User Input;</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Design HERS Verified Dwelling Unit Water Heating Systems Information</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">Require one row of data for each Dwelling Unit Water Heating System name where ResidentialDHWDwellingUnitDistributionType  == StandardDistribution display term Standard Distribution System, StandardDistributionCentralParallelPiping display term Parallel Piping, PointOfUse display term Point of Use, RecirculationNonDemand display term Recirculation System Non-Demand Control, DemandRecirculationManualControl display term Demand Recirculation Manual Control, DemandRecirculationSensorControls display term Demand Recirculation Sensor Controls, Or StandardDistributionInsulatedPipes display term Pipe Insulation Credit</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">This table reports the water heating system features that were specified on the NRCC-PRF-01 compliance document for this project.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableDesignSystem">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="B01_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heating System ID or Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input;</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="B02_ResidentialWaterHeatingSystemType" type="hvac:ResidentialWaterHeatingSystemType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heating System Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input;  Allowed values are: DHW display text: DHW, and HydronicSpaceConditioningDHW display text: Combined Hydronic.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="B03_ResidentialWaterHeaterType" type="hvac:ResidentialWaterHeaterType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heater Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User iselectsfrom list:  BoilerWaterHeater display text: Boiler, HeatPumpWaterHeater display text: Heat Pump,  IndirectWaterHeating display text:  Indirect, InstantaneousLarge display text: Large Instantaneous, InstantaneousSmall display text: Small Instantaneous, StorageLarge display text:Large Storage, StorageSmall display text:Small Storage, StorageCommercial display term: Commercial Storage, StorageResDutyCommercial display term: Residential-Duty Commercial Storage,   StorageConsumer display term: Consumer Storage, InstantaneousCommercial display term: Commercial Instantaneous, InstantaneousConsumer display term: Consumer Instantaneous,</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="B04_WaterHeaterTotalSystemCount" type="hvac:WaterHeaterTotalSystemCount">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"># of Water Heaters in System</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input;</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B05_StorageCapacity" type="hvac:StorageCapacity">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Water Heater Storage Volume (gal)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input;  N/A which is stored in the NotApplicableMessage, is allowed If B04_ResidentialWaterHeaterType == InstantaneousLarge Or InstantaneousSmall</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B05_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Water Heater Storage Volume (gal)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input;  N/A which is stored in the NotApplicableMessage, is allowed If B04_ResidentialWaterHeaterType == InstantaneousLarge Or InstantaneousSmall</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="B06_WaterHeaterFuelSource">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Fuel Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input; Allowed values are: Electricity; NaturalGas; Oil; PropaneGas</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:WaterHeaterFuelSource">
                                             <xsd:enumeration value="Electricity"/>
                                             <xsd:enumeration value="NaturalGas"/>
                                             <xsd:enumeration value="Oil"/>
                                             <xsd:enumeration value="PropaneGas"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B07_WaterHeaterRatedInputUnits" type="hvac:WaterHeaterRatedInputUnits">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_ResidentialWaterHeaterType  == HeatPumpWaterHeater result = N/A stored in NotApplicableMessage Else  Allowed values are:  If fuel type != Electricity result = BtuHr display term Btu/hr,  Else result = kW display term kW</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B07_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_ResidentialWaterHeaterType  == HeatPumpWaterHeater result = N/A stored in NotApplicableMessage Else  Allowed values are:  If fuel type != Electricity result = BtuHr display term Btu/hr,  Else result = kW display term kW</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B08_WaterHeaterElectricFiredRatedInput" type="hvac:WaterHeaterElectricFiredRatedInput">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_ResidentialWaterHeaterType  == HeatPumpWaterHeater result = N/A stored in NotApplicableMessage; Else  User input;</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B08_WaterHeaterGasFiredRatedInput" type="hvac:WaterHeaterGasFiredRatedInput">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_ResidentialWaterHeaterType  == HeatPumpWaterHeater result = N/A stored in NotApplicableMessage; Else  User input;</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B08_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_ResidentialWaterHeaterType  == HeatPumpWaterHeater result = N/A stored in NotApplicableMessage; Else  User input;</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="B09_ResidentialDHWDwellingUnitDistributionType" type="hvac:ResidentialDHWDwellingUnitDistributionType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Dwelling Unit DHW System Distribution Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: StandardDistribution display term Standard Distribution System, StandardDistributionCentralParallelPipingHERS  display term HERS Verified Parallel Piping, PointOfUse display term Point of Use, RecirculationNonDemand display term Recirculation System Non-Demand Control, DemandRecirculationManualControlHERS display term HERS Verified  Demand Recirculation Manual Control, DemandRecirculationSensorControlsHERS display term HERS Verified Demand Recirculation Sensor Controls, StandardDistributionInsulatedPipesHERS display term HERS Verified Pipe Insulation Credit</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="B10_CompactDistributionType" type="hvac:CompactDistributionType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Compact Distrib.</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from NRCC  Allowed values are BasicType display term Basic, Expanded display term Expanded, NotCompact display term None</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="B11_IsDWHR_Installed" type="comp:IsDWHR_Installed">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Drain Water Heat Recovery</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list true result display term Yes; false result display term No</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_C" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Installed HERS Verified Dwelling Unit Water Heating Systems Information</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">Require one row of data for each Water Heating System in Section B</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="C_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">This table reports the water heating system features that were installed in this project.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableInstalledSystem">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="C01_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heating System ID or Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from B01</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C02_ResidentialWaterHeatingSystemType" type="hvac:ResidentialWaterHeatingSystemType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heating System Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from B02</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C03_ResidentialWaterHeaterType" type="hvac:ResidentialWaterHeaterType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heater Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from B03</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C04_WaterHeaterTotalSystemCount" type="hvac:WaterHeaterTotalSystemCount">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"># of Water Heaters in System</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from B04</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="C05_StorageCapacity" type="hvac:StorageCapacity">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Water Heater Storage Volume (gal)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input;  N/A which is stored in the NotApplicableMessage, is allowed If B04_ResidentialWaterHeaterType == InstantaneousLarge Or InstantaneousSmall</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="C05_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Water Heater Storage Volume (gal)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input;  N/A which is stored in the NotApplicableMessage, is allowed If B04_ResidentialWaterHeaterType == InstantaneousLarge Or InstantaneousSmall</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="C06_WaterHeaterFuelSource">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Fuel Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from B06</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:WaterHeaterFuelSource">
                                             <xsd:enumeration value="Electricity"/>
                                             <xsd:enumeration value="NaturalGas"/>
                                             <xsd:enumeration value="Oil"/>
                                             <xsd:enumeration value="PropaneGas"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="C07_WaterHeaterRatedInputUnits" type="hvac:WaterHeaterRatedInputUnits">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference value from B07</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="C07_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference value from B07</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="C08_WaterHeaterElectricFiredRatedInput" type="hvac:WaterHeaterElectricFiredRatedInput">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference value from B08</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="C08_WaterHeaterGasFiredRatedInput" type="hvac:WaterHeaterGasFiredRatedInput">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference value from B08</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="C08_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference value from B08</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="C09_ResidentialDHWDwellingUnitDistributionType" type="hvac:ResidentialDHWDwellingUnitDistributionType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Dwelling Unit DHW System Distribution Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values B09</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C10_CompactDistributionType" type="hvac:CompactDistributionType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Compact Distrib.</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from B10</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C11_IsDWHR_Installed" type="comp:IsDWHR_Installed">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Drain Water Heat Recovery</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from B11</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_D" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Design Dwelling Unit Water Heating Efficiency Information</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="D_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">This table reports the water heater(s) efficiency features specified on the registered NRCC-PRF-01 compliance document for this project.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableDesignEfficiency">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="D01_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heating System ID or Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from B01</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="D02_EfficiencyType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Heating Efficiency Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: EnergyFactor, UniformEnergyFactor, AFUE, ThermalEfficiency</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:EfficiencyType">
                                             <xsd:enumeration value="AFUE"/>
                                             <xsd:enumeration value="EnergyFactor"/>
                                             <xsd:enumeration value="ThermalEfficiency"/>
                                             <xsd:enumeration value="UniformEnergyFactor"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="D03_EnergyFactor" type="hvac:EnergyFactor">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input Energy Factor</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="D03_UniformEnergyFactor" type="hvac:UniformEnergyFactor">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input Uniform Energy Factor</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="D03_WaterHeaterAFUE" type="hvac:WaterHeaterAFUE">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input AFUE</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="D03_ThermalEfficiency" type="hvac:ThermalEfficiency">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input Thermal Efficiency</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="D04_WaterHeatingStandbyLossPercent" type="hvac:WaterHeatingStandbyLossPercent">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Standby Loss (%)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input value If ResidentialWaterHeaterType != StorageSmall, HeatPumpWaterHeater, BoilerWaterHeater, InstantaneousLarge , InstantaneousSmall, Consumer Instantaneous, Commercial Instantaneous, Residential-Duty Commercial Instantaneous, IndirectWaterHeating; Else result = N/A stored in the NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="D04_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Standby Loss (%)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input value If ResidentialWaterHeaterType != StorageSmall, HeatPumpWaterHeater, BoilerWaterHeater, InstantaneousLarge , InstantaneousSmall, Consumer Instantaneous, Commercial Instantaneous, Residential-Duty Commercial Instantaneous, IndirectWaterHeating; Else result = N/A stored in the NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="D05_ExteriorTankInsulation" type="hvac:ExteriorTankInsulation">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exterior Insul. R-Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input value If ResidentialWaterHeaterType != StorageLarge, StorageSmall, HeatPumpWaterHeater, BoilerWaterHeater, InstantaneousLarge , InstantaneousSmall, Consumer Instantaneous, Commercial Instantaneous, Residential-Duty Commercial Instantaneous; Else result = N/A stored in the NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="D05_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exterior Insul. R-Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input value If ResidentialWaterHeaterType != StorageLarge, StorageSmall, HeatPumpWaterHeater, BoilerWaterHeater, InstantaneousLarge , InstantaneousSmall, Consumer Instantaneous, Commercial Instantaneous, Residential-Duty Commercial Instantaneous; Else result = N/A stored in the NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="D06_TankLocation" type="hvac:TankLocation">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tank Location</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User selects from list: Conditioned, Garage.  N/A is allowed if water heater location is not required</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="D06_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tank Location</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User selects from list: Conditioned, Garage.  N/A is allowed if water heater location is not required</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_E" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Installed Dwelling Unit Water Heating Efficiency Information</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="E_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">This table reports the water heating system features that were installed in this project.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableInstalledEfficiency">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="E01_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heating System ID or Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from B01</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="E02_EfficiencyType" type="hvac:EfficiencyType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Heating Efficiency Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from D02</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="E03_EnergyFactor" type="hvac:EnergyFactor">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input; Check: If result GTE D03 result complies; Else flag as non-compliant and do not allow the doc to be registered</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="E03_UniformEnergyFactor" type="hvac:UniformEnergyFactor">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input; Check: If result GTE D03 result complies; Else flag as non-compliant and do not allow the doc to be registered</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="E03_WaterHeaterAFUE" type="hvac:WaterHeaterAFUE">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input; Check: If result GTE D03 result complies; Else flag as non-compliant and do not allow the doc to be registered</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="E03_ThermalEfficiency" type="hvac:ThermalEfficiency">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input; Check: If result GTE D03 result complies; Else flag as non-compliant and do not allow the doc to be registered</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="E04_WaterHeatingStandbyLossPercent" type="hvac:WaterHeatingStandbyLossPercent">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Standby Loss (%)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If D04_NotApplicableMessage == N/A result = N/A stored in NotApplicableMessage; Else User input; Check: If result LTE D04 result complies; Else flag as non-compliant and do not allow the doc to be registered</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="E04_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Standby Loss (%)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If D04_NotApplicableMessage == N/A result = N/A stored in NotApplicableMessage; Else User input; Check: If result LTE D04 result complies; Else flag as non-compliant and do not allow the doc to be registered</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="E05_ExteriorTankInsulation" type="hvac:ExteriorTankInsulation">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exterior Insul. R-Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If D05_NotApplicableMessage == N/A result = N/A stored in NotApplicableMessage; Else User input; Check: If result GTE D05 result complies; Else flag as non-compliant and do not allow the doc to be registered</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="E05_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exterior Insul. R-Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If D05_NotApplicableMessage == N/A result = N/A stored in NotApplicableMessage; Else User input; Check: If result GTE D05 result complies; Else flag as non-compliant and do not allow the doc to be registered</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="E06_TankLocation" type="hvac:TankLocation">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tank Location</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If D06_NotApplicableMessage == N/A result = N/A stored in NotApplicableMessage; Else User input; Check: If result == D06 result complies; Else flag as non-compliant and do not allow the doc to be registered</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="E06_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tank Location</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If D06_NotApplicableMessage == N/A result = N/A stored in NotApplicableMessage; Else User input; Check: If result == D06 result complies; Else flag as non-compliant and do not allow the doc to be registered</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_F" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Installed Water Heater Manufacturer Information</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableInstalledWaterHeater">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="F01_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heating System ID or Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from B01</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="F02_WaterHeaterManufacturer" type="hvac:WaterHeaterManufacturer">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Manufacturer</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input;</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="F03_WaterHeaterModelNumber" type="hvac:WaterHeaterModelNumber">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Model Number</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input;</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_G" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">HERS-Verified Compact Hot Water Distribution (CHWD) (Ra3.6.5)</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is applicable if any water heating system has B10_CompactDistributionType == Expanded.  Require one row for each dwelling identified in section B where B10_CompactDistributionType == Expanded</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="G_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">For dwelling units with multiple systems, enter the master bath distance and kitchen distance to the closet water heater, and enter the average of the furthest fixture to each water heater.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableCompactExpanded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="G01_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">System Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from B01</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G02_TotalStoryCount" type="com:TotalStoryCount">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Number of Stories</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If performance result = N/A stored in NotApplicableMessage; Else user input limited to 1,2 or 3</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G02_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Number of Stories</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If performance result = N/A stored in NotApplicableMessage; Else user input limited to 1,2 or 3</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="G03_WaterHeaterToBath" type="comp:WaterHeaterToBath">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Master Bath distance of furthest fixture to Water Heater in feet</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G04_WaterHeaterToKitchen" type="comp:WaterHeaterToKitchen">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Kitchen distance of furthest fixture to Water Heater in feet</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G05_WaterHeaterToFurthest3rdFixture" type="comp:WaterHeaterToFurthest3rdFixture">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Furthest Third furthest fixture to Water Heater in feet (Avg for multiple water heaters)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G06_WaterHeaterWeightedDistance" type="comp:WaterHeaterWeightedDistance">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Weighted Distance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If B09_ResidentialDHWDwellingUnitDistributionType == StandardDistribution result =  (G03_WaterHeaterToBath * 0.4)  + (G04_WaterHeaterToKitchen *  0.4)  + (G05_WaterHeaterToFurthest3rdFixture * 0.2); Else If B09_ResidentialDHWDwellingUnitDistributionType == DemandRecirculationManualControl result = G05</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G07_WaterHeaterQualificationDistance" type="comp:WaterHeaterQualificationDistance">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Qualification Distance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated result = ((CompactDistCoefA + CompactDistCoefB) *  CFA) / B04_WaterHeaterTotalSystemCount; Values for formula are as follows:  CompactDistCoefA,CompactDistCoefB are found in the Table  4.4.6-2 Coefficients for the Qualification Distance Calculation following section G in layout document;   CFA == conditioned floor area of the dwelling unit from CF1R,</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="G08" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">No hot water piping 
                           <d:gt/>1 inch diameter is allowed.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G09" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Length of 1 inch diameter piping is limited to 8 feet or less.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G10" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Two and three story buildings cannot have hot water distribution piping in the attic, unless the water heater is also located in the attic.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G11" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Eligible recirculating systems must be HERS-Verified Demand Recirculation: Manual Control conforming to RA4.4.17.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G12_DoesSystemComplyWithRequirements" type="comp:DoesSystemComplyWithRequirements">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Verification Status:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Display choices: Pass,  Fail  or N/A</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G13_SectionCorrectionNotes" type="comp:SectionCorrectionNotes" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Correction Notes:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If DoesSystemComplyWithRequirements value is Fail, require that the reason is described in this field</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="GEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met unless otherwise noted in the Verification Status and the Corrections Notes in this table.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_H" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">HERS-Verified Drain Water Heat Recovery System (DWHR - H)</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required If B11_IsDWHR_Installed == true</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableDesignDWHR">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Design DWHR System Information</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="H01_ResidentialDwellingUnitName" type="com:ResidentialDwellingUnitName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">System ID/Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from A01</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H02_DWHR_RatedEffectiveness" type="comp:DWHR_RatedEffectiveness">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Rated Effectiveness</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If performance, user input; Else If prescriptive result = .42   display term greater than or equal to 42%</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="H03_DWHR_Configuration" type="comp:DWHR_Configuration">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Installation Configuration</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If performance, user input; Else If prescriptive result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="H03_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Installation Configuration</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If performance, user input; Else If prescriptive result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="H04_DWHR_PercentShowerServed" type="comp:DWHR_PercentShowerServed">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Percent of shower served by the DWHR device</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If performance, user input; Else If prescriptive result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="H04_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Percent of shower served by the DWHR device</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If performance, user input; Else If prescriptive result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="TableDWHR">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Installed DWHR System Information</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="H05_ResidentialDwellingUnitName" type="com:ResidentialDwellingUnitName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">System ID/Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from A01</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H06_DWHR_Manufacturer" type="comp:DWHR_Manufacturer">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Manufacturer</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H07_DWHR_ModelNumber" type="comp:DWHR_ModelNumber">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Model #</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H08_DWHR_RatedEffectiveness" type="comp:DWHR_RatedEffectiveness">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Rated Effectiveness</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input value GTE H02; Check result GTE 0.42 And LT 1.00</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H09_DWHR_Configuration" type="comp:DWHR_Configuration">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Installation Configuration</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If performance, reference H03; Else If prescriptive user selects from list: EqualFlow display term Equal Flow, UnequalFlowShower display term Unequal to Shower, UnequalFlowWaterHeater display term Unequal to Water Heater</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H10_DWHR_PercentShowerServed" type="comp:DWHR_PercentShowerServed">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Percent of shower served by the DWHR device</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If performance, reference H04; Else If prescriptive user input;  Result is decimal value no units GT 0 LTE 1.00 and  displayed term is percent = result * 100;  Check on displayed term is GT 0 And LT 100</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H11_DWHR_CertifiedByCEC" type="comp:DWHR_CertifiedByCEC">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">DWHR System Certified by CEC (Yes/No)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: Yes (result = true), No (result = false)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="H12" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">For water heating system serving a single dwelling, the DWHR system shall, at the minimum, recover heat from the master bathroom shower and must at least transfer that heat either back to the respective shower(s) or the water heater.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H13" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">For central water heating system serving multiple dwellings, the DWHR system shall, at the minimum, recover heat from half the showers located above the first floor and must at least transfer that heat either back to all the respective showers or the water heater.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H14" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">The DWHR unit(s) shall be installed within 1 degrees of the rated slope.  Sloped DWHR shall have a minimum lengthwise slope of 1 degree.  The lateral level tolerance shall be within plus or minus 1 degree.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H15_DoesSystemComplyWithRequirements" type="comp:DoesSystemComplyWithRequirements">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Verification Status:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Display choices: Pass,  Fail  or N/A</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H16_SectionCorrectionNotes" type="comp:SectionCorrectionNotes" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Correction Notes:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If DoesSystemComplyWithRequirements value is Fail, require that the reason is described in this field</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="HEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met unless otherwise noted in the Verification Status and the Corrections Notes in this table.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_I" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Mandatory Measures For Insulation for Piping and Tanks</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="I01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Equipment shall meet the applicable requirements of the Appliance Efficiency Regulations (Section 110.3(b)1).</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Unfired storage tanks are insulated with an external R-12 or combination of R-16 internal and external Insulation. (Section 110.3(c)4).</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">The following pipes are insulated, to the thicknesses required by Table 120.3A, except for those sections of pipe that are subject to one of the exceptions below:  All domestic hot water pipes shall be insulated as specified in Section 609.11 of the California Plumbing Code.  In addition, the following piping conditions shall have a minimum insulation wall thickness of 1 inch or a minimum insulation R-value of 7.7 (RA4.4.1) 
                           <d:l t="*">
                              <d:list> The first 5 feet (1.5 meters) of cold water pipes from the storage tank. </d:list>
                              <d:list>All piping with a nominal diameter of 3/4 inch (19 millimeter) and less than 1 inch.. </d:list>
                              <d:list>All hot water pipes from the heating source to the kitchen fixtures.</d:list>
                              <d:list>Piping from the heating source to storage tank or between tanks. </d:list>
                              <d:list>All piping associated with a recirculation system. </d:list>
                              <d:list>All underground piping. </d:list>
                              <d:list> Insulation buried below grade must be installed in a waterproof and non-crushable casing or sleeve. </d:list>
                              <d:list>Piping that penetrates framing members shall not be required to have pipe insulation for the distance of the framing penetration.  Piping that penetrates metal framing shall use grommets, plugs, wrapping or other insulating material to assure that no contact is made with the metal framing. Insulation shall butt securely against all framing members.</d:list>
                              <d:list> Piping installed in interior or exterior walls that is surrounded on all sides by at least 1 inch (2.5 cm) of insulation. </d:list>
                              <d:list> Piping installed in crawl space with a minimum of 1 inch (2.5 cm) of crawl space insulation above and below. </d:list>
                              <d:list>Piping installed in attics with a minimum of 4 inches (10 cm) of attic insulation on top </d:list>
                              <d:list>Piping insulation shall fit tightly and all elbows and tees shall be fully insulated. </d:list>
                           </d:l>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">For Gas or Propane Water Heaters: Ensure the following are installed (Section 150.0(n))  
                           <d:l t="#">
                              <d:list>A dedicated 120V, 20A electrical receptacle connected to the electrical panel with a 120/240V 3 conductor, 10AWG copper branch circuit, within 3 feet from the water heater and accessible with no obstructions 
                                 <d:l t="#">
                                    <d:list>The conductor shall be labeled with the word "Spare" on both ends; and </d:list>
                                    <d:list> A reserved single pole circuit breaker space next to the circuit breaker next to the branch circuit  labeled "Future" 240V shall be provided. </d:list>
                                 </d:l>
                              </d:list>
                              <d:list> A Category III or IV vent, or a Type B vent with straight pipe between outside and water heater  </d:list>
                              <d:list>A condensate drain no more than 2 inches higher than the base on water heater for natural draining   </d:list>
                              <d:list>A gas supply line with capacity of at least 200,000 Btu/Hr</d:list>
                           </d:l>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="IEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met unless otherwise noted in the Verification Status and the Corrections Notes in this table.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_J" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">HERS-Verified Pipe Insulation Credit Requirements</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is applicable if there is at least one water heating system that has B09_ResidentialDHWDwellingUnitDistributionType == StandardDistributionInsulatedPipesHERS</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="J_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Systems that utilize this distribution type shall comply with these requirements</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="J01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">All hot water piping shall comply with the insulation requirements in Table 120.3-A. (RA 4.4.14)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="J02_DoesSystemComplyWithRequirements" type="comp:DoesSystemComplyWithRequirements">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Verification Status:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Display choices: Pass,  Fail  or N/A</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="J03_SectionCorrectionNotes" type="comp:SectionCorrectionNotes" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Correction Notes:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If DoesSystemComplyWithRequirements value is Fail, require that the reason is described in this field</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="JEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met unless otherwise noted in the Verification Status and the Corrections Notes in this table.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_K" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">HERS-Verified Parallel Piping Requirements</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is applicable if there is at least one water heating system that has B09  ResidentialDHWDwellingUnitDistributionType ==  StandardDistributionCentralParallelPipingHERS</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="K_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Systems that utilize this distribution type shall comply with these requirements</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Each central manifold has 5 feet or less of pipe between manifold and water heater. (RA 4.4.15)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">For manifolds that include valves, the manifold must be readily accessible in accordance with the plumbing code. (RA 4.4.4)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Hot water distribution system piping from the manifold to the fixtures and appliances must take the most direct path.  For example, piping from a second story manifold cannot supply the first floor.  (RA 4.4.4)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">The hot water distribution piping must be separated by at least 2 inches from any other hot water supply piping, and at least 6 inches from any cold water supply piping.  Alternatively, the hot water supply piping must be insulated to the thicknesses shown in TABLE 120.3-A. (RA 4.4.4)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K05_DoesSystemComplyWithRequirements" type="comp:DoesSystemComplyWithRequirements">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Verification Status:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Display choices: Pass,  Fail  or N/A</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K06_SectionCorrectionNotes" type="comp:SectionCorrectionNotes" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Correction Notes:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If DoesSystemComplyWithRequirements value is Fail, require that the reason is described in this field</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="KEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met unless otherwise noted in the Verification Status and the Corrections Notes in this table.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_L" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">HERS-Verified Demand Recirculation Manual Control Requirements (R-DRmc-H) (RA3.6.6.)</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is applicable if there is at least one water heating system that has B09_ResidentialDHWDwellingUnitDistributionType == DemandRecirculationManualControlHERS</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="L_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Systems that utilize this distribution type shall comply with these requirements</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">The system operates on-demand, meaning that the pump begins to operate shortly before or immediately after hot water draw begins, and stops when the return water temperature reaches a certain threshold value. (RA4.4.9/RA4.4.13)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">After the pump has been activated, the controls shall allow the pump to operate until the water temperature at the thermo-sensor rises to one of the following values: (RA4.4.9(f)/RA4.4.13(d)) 
                           <d:l t="*">
                              <d:list>  Not more than 10
                                 <d:deg/> F ( 5.6
                                 <d:deg/> C ) above the initial temperature of the water in the pipe     </d:list>
                              <d:list> Not more than 102
                                 <d:deg/> F (38.9
                                 <d:deg/> C). </d:list>
                           </d:l>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">The controls shall limit pump operation to a maximum of 5 minutes for single dwellings, and 10 minutes for multiple dwellings, following any activation. This is provided in the event that the normal means of shutting off the pump have failed. (RA4.4.9(f)3/RA4.4.13(d)3)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Pump and control placement shall meet one of the following criteria: (RA4.4.9(d)/RA4.4.13(b)) 
                           <d:l t="*">
                              <d:list>  When a dedicated return line has been installed the pump, controls and thermo-sensor are installed at the end of the supply portion of the recirculation loop; or </d:list>
                              <d:list>  The pump and controls are installed on the dedicated return line near the water heater and the thermo-sensor is installed in an accessible location as close to the end of the supply portion of the recirculation loop as possible, or  </d:list>
                              <d:list>   When the cold water line is used as the return, the pump, demand controls and thermo-sensor shall be installed in an accessible location at the end of supply portion of the hot water distribution line (typically under a sink). </d:list>
                           </d:l>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L05" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Insulation is not required on the cold water line when it is used as the return. (RA4.4.9(e)/RA4.4.13(c))</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L06" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Each control shall have standby power of 1 Watt or less.  Controls may be located in individual units or on the loop. Controls may be activated by wired or wireless mechanisms, including buttons, motion sensors, door switches and flow switches. (RA4.4.9(c)/RA4.4.13(a))</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L07" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">If more than one loop installed each loop shall have its own pump and controls</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L08" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Automatic air release valve is installed on the inlet side of the recirculation pump per Section 110.3(c)5A.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L09" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">A check valve, or similar device, is located between the recirculation pump and the water heater per Section 110.3(c)5B.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L10" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Hose bibb is installed between the pump and the water heating equipment with an isolation valve between the hose bibb and the water heating equipment per Section 110.3(c)5C.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L11" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Isolation valves are installed on both sides of the pump. One of the isolation valves may be the same isolation valve as in item 10 above per Section 110.3(c)5D.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L12" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">The cold water supply piping and the recirculation loop piping is not connected to the hot water storage tank drain port per Section 110.3(c)5E.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L13" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">A check valve is installed on the cold water supply line between the hot water system and the next closest tee on the cold water supply line per Section 110.3(c)5F.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L14_DoesSystemComplyWithRequirements" type="comp:DoesSystemComplyWithRequirements">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Verification Status:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Display choices: Pass,  Fail  or N/A</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L15_SectionCorrectionNotes" type="comp:SectionCorrectionNotes" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Correction Notes:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If DoesSystemComplyWithRequirements value is Fail, require that the reason is described in this field</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met unless otherwise noted in the Verification Status and the Corrections Notes in this table.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_M" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">HERS-Verified Demand Recirculation Sensor Control Requirements (RDRsc-H) (RA3.6.7)</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is applicable if there is at least one water heating system that has B09_ResidentialDHWDwellingUnitDistributionType equal to == DemandRecirculationSensorControlsHERS</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="M_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Systems that utilize this distribution type shall comply with these requirements</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="M01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">The system operates "on-demand", meaning that the pump begins to operate shortly before or immediately after hot water draw begins, and stops when the return water temperature reaches a certain threshold value. (RA4.4.13)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="M02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">After the pump has been activated, the controls shall allow the pump to operate until the water temperature at the thermo-sensor rises to one of the following values: (RA4.4.13)   
                           <d:l t="*">
                              <d:list> Not more than 10
                                 <d:deg/>F ( 5.6
                                 <d:deg/>C ) above the initial temperature of the water in the pipe  </d:list>
                              <d:list>Not more than 102
                                 <d:deg/>F (38.9
                                 <d:deg/>C).</d:list>
                           </d:l>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="M03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">The controls shall limit pump operation to a maximum of 5 minutes for single dwellings, and 10 minutes for multiple dwellings,  following any activation. This is provided in the event that the normal means of shutting off the pump have failed. (RA4.4.13)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="M04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Pump and control placement shall meet one of the following criteria: (RA4.4.13)    
                           <d:l t="*">
                              <d:list> When a dedicated return line has been installed the pump, controls and thermo-sensor are installed at the end of the supply portion of the recirculation loop; or   </d:list>
                              <d:list>The pump and controls are installed on the dedicated return line near the water heater and the thermosensor is installed in an accessible location as close to the end of the supply portion of the recirculation loop as possible, or  </d:list>
                              <d:list>When the cold water line is used as the return, the pump, demand controls and thermo-sensor shall be installed in an accessible location at the end of supply portion of the hot water distribution line (typically under a sink).</d:list>
                           </d:l>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="M05" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Insulation is not required on the cold water line when it is used as the return. (RA4.4.13)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="M06" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Each control shall have standby power of 1 Watt or less.  Controls may be located in individual units or on the loop. Controls may be activated by wired or wireless mechanisms, including buttons, motion sensors, door switches and flow switches. (RA4.4.10(c)/RA4.4.13(a))</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="M07" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">If more than one loop installed each loop shall have its own pump and controls</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="M08" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Automatic air release valve is installed on the inlet side of the recirculation pump per Section 110.3(c)5A.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="M09" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">A check valve is located between the recirculation pump and the water heater per Section 110.3(c)5B.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="M10" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Hose bibb is installed between the pump and the water heating equipment with an isolation valve between the hose bibb and the water heating equipment per Section 110.3(c)5C.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="M11" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Isolation valves are installed on both sides of the pump. One of the isolation valves may be the same isolation valve as in item 10 above per Section 110.3(c)5D.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="M12" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">The cold water supply piping and the recirculation loop piping is not connected to the hot water storage tank drain port per Section 110.3(c)5E.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="M13" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
